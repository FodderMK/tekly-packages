<head>
    <title>{{title}}</title>
    <meta charset="utf-8">

    <link href='https://unpkg.com/boxicons@2.1.4/css/boxicons.min.css' rel='stylesheet'>
    
    <!-- JQuery -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.7.1/dist/jquery.min.js"></script>
    
    <!-- Fancy Tree -->
    <link href="https://cdn.jsdelivr.net/npm/jquery.fancytree@2.27/dist/skin-win8/ui.fancytree.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/jquery.fancytree@2.27/dist/jquery.fancytree-all-deps.min.js"></script>

    <!-- Htmx -->
    <script src="https://unpkg.com/htmx.org@1.9.12"></script>

    <!-- GridStack -->
    <link href="https://unpkg.com/gridstack/dist/gridstack.min.css" rel="stylesheet" />
    <script src="https://unpkg.com/gridstack/dist/gridstack-all.js"></script>

    <!-- Liquid -->
    <script src="https://cdn.jsdelivr.net/npm/liquidjs/dist/liquid.browser.min.js"></script>
    
    <script type="module" src="/Tinker/components.jst"></script>
    
    <!-- All custom CSS -->
    {%- for css in Tinker.Css -%}
        <link rel="stylesheet" type="text/css" href="{{ css }}">
    {%- endfor -%}
    
    <script>
        TINKER_WS_PORT = {{ Tinker.WebSocketPort }};
        
        function triggerReload(element) {
            let event = new Event("reload", {bubbles: true});
            element.dispatchEvent(event);
        }
    </script>
</head>