{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "rules": {
        "arrow-parens": [
            false
        ],
        "max-line-length": [true, 140],
        "member-ordering": [
            true,
            "fields-first"
        ],
        "no-console": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "prefer-for-of": [
            false
        ],
        "quotemark": [
            true,
            "single"
        ],
        "radix": [
            false
        ],
        "trailing-comma": [
            false
		],
		"indent": [true, "tabs"]
    },
    "rulesDirectory": []
}